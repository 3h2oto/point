/* pages/cartypes/cartypes.wxss */
.title{
  width:100%;
  height:30px;
  background-color: #9ac81f;
}
.cartype{
   width:92%;
   margin-left:4%;
  height:70px;
  display: flex;
 border-top: 2px solid #EBEBEB;
 border-radius: 5px 5px 5px 5px;
  margin-top:3%;
  background-color: white;
  box-shadow: 0px 2px 5px 5px #ebebeb;
}
.main{
width:100%;
height:100%;
background-color: #F8F8F8;
}

.page {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.usrimage {
  width: 10%;
  margin-left: 5%;
  margin-right: 5%;
  background-size: 100%;
}

.navbar {
  flex: none;
  display: flex;
  background: #fff;
    color: #e0e3da;
}

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}

.navbar .item.active {
  color: #000;
}

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 45%;
  right: 0;
  height: 4rpx;
  width: 15%;
  background: #9ac81f;
}
